{
  "name": "ixfx-play",
  "version": "1.0.0",
  "description": "",
  "browserslist": "last 2 Chrome versions",
  "scripts": {
    "start": "npm run postinstall && concurrently npm:watch:*",
    "build": "npm run postinstall && tsc && npm run static",
    "check": "tsc --noEmit",
    "static": "cpx \"src/**/*.{html,css,js}\" docs",
    "postinstall": "copyfiles --up 3 node_modules/ixfx/dist/**.* docs/ixfx",
    "watch:tsc": "tsc --watch",
    "watch:static": "cpx \"src/**/*.{html,css,js}\" docs --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "copyfiles": "^2.4.1",
    "cpx2": "^5.0.0",
    "ixfx": "^0.10.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "type": "module",
  "targets": {
    "main": {
      "includeNodeModules": [
        "ixfx"
      ]
    }
  }
}
